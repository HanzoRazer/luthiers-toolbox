{
  "_meta": {
    "tier": "edition",
    "edition": "parametric",
    "category": "guitar_templates",
    "version": "1.0.0",
    "description": "Parametric guitar templates with editable constraints for ltb-parametric"
  },
  "templates": {
    "strat_parametric": {
      "id": "strat_parametric",
      "name": "Stratocaster Parametric",
      "base_template": "stratocaster",
      "constraints": {
        "body_length": {"min_mm": 380, "max_mm": 420, "default_mm": 396},
        "body_width": {"min_mm": 300, "max_mm": 340, "default_mm": 318},
        "body_thickness": {"min_mm": 40, "max_mm": 50, "default_mm": 44.5},
        "horn_length": {"min_mm": 100, "max_mm": 140, "default_mm": 120},
        "waist_depth": {"min_mm": 60, "max_mm": 90, "default_mm": 75},
        "belly_cut_depth": {"min_mm": 0, "max_mm": 15, "default_mm": 10},
        "forearm_contour_depth": {"min_mm": 0, "max_mm": 12, "default_mm": 8}
      },
      "linked_parameters": [
        {"if": "body_width > 330", "then": "horn_length >= 130"},
        {"if": "body_thickness < 42", "then": "belly_cut_depth <= 8"}
      ]
    },
    "tele_parametric": {
      "id": "tele_parametric",
      "name": "Telecaster Parametric",
      "base_template": "telecaster",
      "constraints": {
        "body_length": {"min_mm": 380, "max_mm": 420, "default_mm": 396},
        "body_width": {"min_mm": 300, "max_mm": 340, "default_mm": 318},
        "body_thickness": {"min_mm": 40, "max_mm": 50, "default_mm": 44.5},
        "horn_length": {"min_mm": 80, "max_mm": 120, "default_mm": 100},
        "lower_bout_radius": {"min_mm": 140, "max_mm": 180, "default_mm": 160}
      }
    },
    "lp_parametric": {
      "id": "lp_parametric",
      "name": "Les Paul Parametric",
      "base_template": "les_paul",
      "constraints": {
        "body_length": {"min_mm": 420, "max_mm": 470, "default_mm": 444},
        "body_width": {"min_mm": 310, "max_mm": 350, "default_mm": 330},
        "body_thickness": {"min_mm": 45, "max_mm": 55, "default_mm": 50},
        "top_carve_radius": {"min_mm": 250, "max_mm": 400, "default_mm": 305},
        "top_thickness": {"min_mm": 10, "max_mm": 16, "default_mm": 12},
        "cutaway_depth": {"min_mm": 30, "max_mm": 50, "default_mm": 40}
      }
    },
    "dreadnought_parametric": {
      "id": "dreadnought_parametric",
      "name": "Dreadnought Acoustic Parametric",
      "base_template": "dreadnought",
      "constraints": {
        "lower_bout": {"min_mm": 380, "max_mm": 420, "default_mm": 396},
        "upper_bout": {"min_mm": 270, "max_mm": 310, "default_mm": 286},
        "waist": {"min_mm": 260, "max_mm": 300, "default_mm": 279},
        "body_depth": {"min_mm": 100, "max_mm": 130, "default_mm": 119},
        "soundhole_diameter": {"min_mm": 90, "max_mm": 110, "default_mm": 102},
        "soundhole_position": {"min_mm": 80, "max_mm": 100, "default_mm": 90}
      }
    }
  },
  "parametric_constraints": {
    "scale_body_ratio": {
      "description": "Body length should be proportional to scale length",
      "formula": "body_length = scale_length * 0.61"
    },
    "pickup_spacing": {
      "description": "Pickup positions relative to bridge",
      "bridge_pickup_mm": 20,
      "middle_pickup_mm": 95,
      "neck_pickup_mm": 165
    }
  }
}
